<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="complex_robot">
  <xacro:include filename="$(find robot_state_publisher)/urdf/test-desc.urdf.xacro"/>
  <xacro:include filename="$(find roboclaw_joint_driver)/urdf/roboclaw_linear.urdf.xacro"/>

  <link name="ref_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="${length} ${length} ${length}"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
  </link>

</robot>
