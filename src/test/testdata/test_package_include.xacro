<?xml version="1.0"?>
<robot name="test_package_include" xmlns:xacro="http://www.ros.org/wiki/xacro">
  
  <!-- Include using $(find package) syntax -->
  <xacro:include filename="$(find test_package)/urdf/included_with_args.xacro"/>
  
  <!-- Main file arguments -->
  <xacro:arg name="main_arg" default="main_value"/>
  
  <link name="base_link">
    <visual>
      <geometry>
        <box size="1 1 1"/>
      </geometry>
    </visual>
  </link>
  
</robot>
